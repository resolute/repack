{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "outDir": ".",
        "pretty": true,
        "resolveJsonModule": true,
        "sourceMap": false,
        "strict": true,
        "stripInternal": true,
        "target": "es2018"
    },
    "include": [
        "src"
    ],
    "exclude": []
}
